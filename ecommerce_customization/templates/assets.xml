<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--=== Inclusion of assets files in frontend ===-->
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/css"  href="/ecommerce_customization/static/src/scss/website_preorder.scss" />
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/ecommerce_customization/static/src/js/website_preorder.js"/>
        </xpath>
    </template>
</odoo>
